{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "WANDB_PROJECT": "auv-dreamer-debug",
                "WANDB_API_KEY": "{env:WANDB_API_KEY}"
            }
        },
        {
            "name": "Python: Hello gym-auv",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/playground/hello_gym_auv.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Current File - all code",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "WANDB_PROJECT": "auv-dreamer-debug",
                "WANDB_API_KEY": "{env:WANDB_API_KEY}"
            }
            
        },
        {
            "name": "Python: Current File - pick process",
            "type": "python",
            "request": "attach",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "processId": "${command:pickProcess}"
        },
    ]
}